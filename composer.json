{
    "name": "ad7six/shadow-translate",
    "description": "Manage translated content with shadow tables",
	"type":"cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Dawson",
            "email": "andydawson76@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "cakephp/cakephp": "3.0.*-dev",
        "cakephp/plugin-installer": "*"
    },
    "autoload": {
        "psr-4": {
            "ShadowTranslate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShadowTranslate\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
